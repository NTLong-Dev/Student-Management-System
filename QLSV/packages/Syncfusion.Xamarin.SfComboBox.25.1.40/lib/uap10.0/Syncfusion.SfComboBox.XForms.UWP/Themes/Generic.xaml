    <Style BasedOn="{StaticResource DefaultComboBoxStyle}" TargetType="local:SfComboBox" />

    <Style x:Key="DefaultTokenItemStyle" TargetType="local:TokenItem">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:TokenItem">
                    <Border
                        x:Name="TokenBorder"
                        Height="{TemplateBinding Height}"
                        Margin="4,2,0,2">
                        <Border.Resources>
                            <Style x:Name="TokenCloseStyle" TargetType="Button">
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="Button">
                                            <ContentPresenter
                                                Margin="2"
                                                HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                                Content="{TemplateBinding Content}"
                                                ContentTemplate="{TemplateBinding ContentTemplate}" />
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </Border.Resources>
                        <Grid x:Name="TokenGrid" Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="auto" />
                            </Grid.ColumnDefinitions>
                            <Image
                                x:Name="TokenImage"
                                MaxHeight="20"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Source="{TemplateBinding Image}" />
                            <TextBlock
                                x:Name="TokenTextBlock"
                                Grid.Column="1"
                                Margin="2,0,2,3"
                                VerticalAlignment="Center"
                                Text="{TemplateBinding Item}" />
                            <Button
                                x:Name="TokenCloseButton"
                                Grid.Column="2"
                                Margin="2"
                                Background="Transparent"
                                CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}}"
                                Style="{StaticResource TokenCloseStyle}">
                                <Button.Content>
                                    <TextBlock
                                        x:Name="TokenButtonContent"
                                        VerticalAlignment="Center"
                                        FontFamily="Segoe MDL2 Assets"
                                        FontSize="10"
                                        Foreground="#787878"
                                        Text="&#xE711;" />
                                </Button.Content>
                            </Button>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style BasedOn="{StaticResource DefaultTokenItemStyle}" TargetType="local:TokenItem" />

    <ControlTemplate x:Key="EditableVerticalMultiSelectionTemplate" TargetType="local:SfComboBox">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid Grid.Row="1">
                <Popup x:Name="PART_Popup" IsOpen="{TemplateBinding IsSuggestionOpen}">
                    <Border
                        x:Name="DropDown"
                        BorderBrush="{TemplateBinding DropDownBorderColor}"
                        BorderThickness="1"
                        CornerRadius="{Binding DropDownCornerRadius, Converter={StaticResource DoubleToCornerRadiusConverter}, RelativeSource={RelativeSource Mode=TemplatedParent}}">

                        <Grid
                            x:Name="DropDownGrid"
                            Background="{TemplateBinding DropDownBackground}"
                            CornerRadius="{Binding DropDownCornerRadius, Converter={StaticResource DoubleToCornerRadiusConverter}, RelativeSource={RelativeSource Mode=TemplatedParent}}">
                            <Grid.RowDefinitions>
                                <RowDefinition x:Name="HeaderRow" Height="Auto" />
                                <RowDefinition Height="*" />
                                <RowDefinition x:Name="FooterRow" Height="Auto" />
                            </Grid.RowDefinitions>

                            <Grid Grid.Row="0">
                                <ContentControl
                                    x:Name="DropDownHeader"
                                    HorizontalAlignment="Stretch"
                                    HorizontalContentAlignment="Stretch" />
                            </Grid>
                            <Grid Grid.Row="1">
                                <local:SuggestionBox
                                    x:Name="PART_SuggestionBox"
                                    MaxHeight="{TemplateBinding MaxDropDownHeight}"
                                    Background="{TemplateBinding DropDownBackground}"
                                    BorderBrush="{TemplateBinding DropDownBackground}"
                                    FontFamily="{TemplateBinding SuggestionItemFontFamily}"
                                    FontSize="{TemplateBinding SuggestionItemFontSize}"
                                    FontStyle="{TemplateBinding SuggestionItemFontStyle}"
                                    FontWeight="{TemplateBinding SuggestionItemFontWeight}"
                                    Foreground="{TemplateBinding SuggestionItemForeground}"
                                    ItemTemplate="{TemplateBinding ComboBoxItemTemplate}"
