                                Storyboard.TargetProperty="Opacity"
                                To="1"
                                Duration="0" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="NoError" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
